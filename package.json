{
  "name": "immpatch",
  "version": "0.1.0",
  "description": "Update immutable.js objects using JSON Patch operations",
  "keywords": [
    "json",
    "patch",
    "immutable",
    "immutable.js"
  ],
  "homepage": "https://github.com/zaim/immpatch",
  "bugs": {
    "url": "https://github.com/zaim/immpatch/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Zaim Bakar"
  },
  "main": "index",
  "repository": {
    "type": "git",
    "url": "git://github.com/zaim/immpatch.git"
  },
  "scripts": {
    "lint": "jshint --reporter node_modules/jshint-stylish/stylish src test",
    "compile": "rimraf lib && 6to5 src --out-dir lib",
    "precompile": "npm run lint",
    "test": "mocha",
    "pretest": "npm run compile",
    "testcov": "istanbul cover _mocha -- --reporter dot",
    "pretestcov": "npm run pretest",
    "testci": "mocha"
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 2.0.0"
  },
  "dependencies": {
    "debug": "^2.1.1",
    "immutable": "^3.6.2",
    "json-pointer": "^0.2.2"
  },
  "devDependencies": {
    "6to5": "^2.12.6",
    "chai": "^1.10.0",
    "istanbul": "^0.3.5",
    "jiff": "^0.7.0",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.1.0",
    "rimraf": "^2.2.8"
  }
}
